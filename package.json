{
  "name": "@retypewriter/monorepo",
  "version": "0.1.4",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "vite playground",
    "play": "vite playground",
    "stub": "pnpm -r run stub",
    "lint": "eslint .",
    "release": "bumpp package.json packages/*/package.json && pnpm -r publish --access public --no-git-checks && pnpm -r run publish",
    "test": "vitest",
    "vscode": "nr -C packages/vscode dev",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.7.0",
    "@antfu/ni": "^0.21.12",
    "@babel/types": "^7.24.0",
    "@retypewriter/cli": "workspace:./packages/cli",
    "@types/diff-match-patch": "^1.0.36",
    "@types/node": "^20.11.24",
    "@vscode/vsce": "^2.24.0",
    "@vueuse/core": "^10.9.0",
    "bumpp": "^9.3.1",
    "c8": "^9.1.0",
    "diff-match-patch": "^1.0.5",
    "eslint": "^8.57.0",
    "esno": "^4.0.0",
    "jiti": "^1.21.0",
    "pnpm": "^8.15.4",
    "retypewriter": "workspace:./packages/core",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.3"
  }
}
