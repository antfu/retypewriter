{
  "name": "@retypewriter/monorepo",
  "version": "0.0.2",
  "private": true,
  "packageManager": "pnpm@7.12.0",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "vite playground",
    "play": "vite playground",
    "stub": "pnpm -r run stub",
    "lint": "eslint .",
    "release": "bumpp package.json packages/*/package.json && pnpm -r publish --access public --no-git-checks && pnpm -r run publish",
    "test": "vitest",
    "vscode": "nr -C packages/vscode dev",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/ni": "^0.18.2",
    "@babel/types": "^7.19.3",
    "@retypewriter/cli": "workspace:./packages/cli",
    "@types/diff-match-patch": "^1.0.32",
    "@types/node": "^18.8.2",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "diff-match-patch": "^1.0.5",
    "eslint": "^8.24.0",
    "esno": "^0.16.3",
    "jiti": "^1.16.0",
    "pnpm": "^7.13.2",
    "retypewriter": "workspace:./packages/core",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11",
    "vite": "^3.1.4",
    "vitest": "^0.23.4",
    "vue-tsc": "^0.40.13"
  }
}
